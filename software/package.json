{
  "name": "@platforma-open/milaboratories.java-debug.software",
  "version": "1.0.0",
  "description": "JAVA commands for showing execution environment bounds",
  "scripts": {
    "build-skip": "echo 'Fake build script for CI workflow optimisation'",

    "build": "pl-pkg build",
    "sign": "pl-pkg sign packages --sign-command='[\"gcloud-kms-sign\", \"{pkg}\", \"{pkg}.sig\"]'",

    "prepublishOnly": "npm run build && npm run sign && pl-pkg publish packages --skip-existing-packages"
  },
  "block-software": {
    "entrypoints": {
      "flags": {
        "binary": {
          "artifact": {
            "registry": "platforma-open",
            "name": "milaboratories/java-debug",
            "type": "java",
            "environment": "@platforma-open/milaboratories.runenv-java-corretto:21.0.2.13.1",
            "root": "./src/"
          },
          "cmd": [
            "java",
            "-XX:MaxRAMPercentage=100.0",
            "-XX:+PrintFlagsFinal",
            "-version"
          ]
        }
      }
    }
  },
  "files": [
    "dist/"
  ],
  "license": "UNLICENSED",
  "devDependencies": {
    "@platforma-sdk/package-builder": "^2.10.6",
    "@platforma-open/milaboratories.runenv-java-corretto": "^1.0.2",
    "unzipper": "^0.12.3"
  }
}
